{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
		    "path" : "pages/home/home",
		    "style" : {
		        "navigationBarTitleText": "%home.name%"
		    }
		},
		{
		    "path" : "pages/category/category",
		    "style" : {
		        "navigationBarTitleText": "%category.name%"
		    }
		},
		{
		    "path" : "pages/user/user",
		    "style" : {
		        "navigationBarTitleText": "%user.name%"
		    }
		},
		{			"path": "pages/login/login",			"style" : {				"navigationBarTitleText": "%login.name%"			}		},
	    {
            "path" : "pages/register/register",
            "style" : {				"navigationBarTitleText": "%register.name%"			}
        }
    ],
	"globalStyle": {
		"navigationBarTextStyle": "black",				"navigationBarBackgroundColor": "#FFFFFF",		"backgroundColor": "#FFFFFF"
	},	"tabBar": {		"color": "#a2a9a8",		"selectedColor": "#30C6B3",		"borderStyle": "black",		"backgroundColor": "#FFFFFF",		"list": [
			{
				"pagePath": "pages/home/home",
				"iconPath": "static/imgs/home_default.png",
				"selectedIconPath": "static/imgs/home_active.png",
				"text": "%home.name%"
			},
			{
				"pagePath": "pages/category/category",
				"iconPath": "static/imgs/category_default.png",
				"selectedIconPath": "static/imgs/category_active.png",
				"text": "%category.name%"
			},			{				"pagePath": "pages/user/user",				"iconPath": "static/imgs/user_default.png",				"selectedIconPath": "static/imgs/user_active.png",				"text": "%user.name%"			}		]	}
}
